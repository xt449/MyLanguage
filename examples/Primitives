bool aBoolean = false;
ubyte anUnsignedByte = 0;
byte aSignedByte = 0;
char aCharacter = '\u0000';
ushort anUnsignedShort = 0;
short aSignedShort = 0;
uint anUnsignedInteger = 0;
int aSignedInteger = 0;
ulong anUnsignedLong = 0;
long aSignedLong = 0;
float aFloat = 0F;
double aDouble = 0D;
char[] aCharacterArray = "example";

bool* aBooleanPointer = new bool(false);
ubyte* anUnsignedBytePointer = new ubyte(0);
byte* aSignedBytePointer = new byte(0);
char* aCharacterPointer = new char('\u0000');
ushort* anUnsignedShortPointer = new ushort(0);
short* aSignedShortPointer =  new short(0);
uint* anUnsignedIntegerPointer = new uint(0);
int* aSignedIntegerPointer = new int(0);
ulong* anUnsignedLongPointer = new ulong(0);
long* aSignedLongPointer = new long(0);
float* aFloatPointer = new float(0F);
double* aDoublePointer = new double(0D);
char[][] aCharacterArrayPointer = { "example" };

delete(aBooleanPointer);
delete(anUnsignedBytePointer);
delete(aSignedBytePointer);
delete(aCharacterPointer);
delete(anUnsignedShortPointer);
delete(aSignedShortPointer);
delete(anUnsignedIntegerPointer);
delete(aSignedIntegerPointer);
delete(anUnsignedLongPointer);
delete(aSignedLongPointer);
delete(aFloatPointer);
delete(aDoublePointer);
delete(aCharacterArrayPointer);
